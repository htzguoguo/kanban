{
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-decorators",
    "transform-decorators-legacy",
      ["import", {
          "libraryName": "antd",
          "style": "css"
      }]

  ],
  "presets": [
    "es2015",
    "react",
    "flow"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
